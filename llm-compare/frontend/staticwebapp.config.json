{
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".ts": "application/javascript",
    ".tsx": "application/javascript"
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  }
} 